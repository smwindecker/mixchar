  pages = {740-745},
  file = {/Users/windeckers/Zotero/storage/PSZFDAAB/Adler et al. - 2014 - Functional traits explain variation in plant life .pdf;/Users/windeckers/Zotero/storage/MGJSPT47/740.html},
